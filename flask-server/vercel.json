{
    "version": 2,
    "builds": [
        {
            "src": "./server.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        { "handle": "filesystem" },
        {
          "src": "/members",
          "dest": "/backend/members"
        },
        {
          "src": "/upload-video",
          "dest": "backend/upload-video.py"
        },
        {
          "src": "/(.*)",
          "dest": "/index.html"
        }
      ],
      "functions": {
        "backend/**/*.py": {
          "runtime": "python3.8"
        }
      }
}